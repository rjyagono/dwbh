 <script>
      $(document).ready(function() {
          $("#slider").slider({
              range: "min",
              animate: true,
              value:1,
              min: 5000,
              max: 1000000,
              step: 10,
              slide: function(event, ui) {
                update(1,ui.value); //changed
              }
          });

          $("#slider2").slider({
              range: "min",
              animate: true,
              value:1,
              min: 1,
              max: 48,
              step: 1,
              slide: function(event, ui) {
                update(2,ui.value); //changed
              }
          });
          

          //Added, set initial value.
          $("#amount").val(5000);
          $("#duration").val(1);
          $("#amount-label").text(5000);
          $("#duration-label").text(1);
          
          $('#myForm input').on('change', function() {
			   update(); 
			});

         
          update();
      });

          $('#myFormResult input').on('keyup', function() {
               $( "#amount" ).val($("#amount").val());
               $( "#duration" ).val($("#duration").val());
			   update(); 
			});
         
      //changed. now with parameter
      function update(slider,val) {
        //changed. Now, directly take value from ui.value. if not set (initial, will use current value.)
        var $amount = slider == 1?val:$("#amount").val();
        var $duration = slider == 2?val:$("#duration").val();
        var $interest = $('input[name=interest]:checked', '#myForm').val() == undefined ? $("#interest").val() : $('input[name=interest]:checked', '#myForm').val();

        /* commented
        $amount = $( "#slider" ).slider( "value" );
        $duration = $( "#slider2" ).slider( "value" );
         */
         
         $interest_amount = (parseInt($amount)+((parseInt($amount) * parseFloat($interest))*$duration));
         $interest_amount_total = "â‚±" + Math.round($interest_amount/$duration).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, '$1,');
         
         //$total = "P" + ($amount * $duration);
         $( "#amount" ).val(parseFloat($amount));
         $( "#amount-label" ).text($amount);
         $( "#duration" ).val($duration);
         $( "#duration-label" ).text($duration);
         $( "#total" ).val($interest_amount_total);
         $( "#total-label" ).text($interest_amount_total);

         $('#slider a').html('<label><span class="glyphicon glyphicon-chevron-left"></span> '+$amount+' <span class="glyphicon glyphicon-chevron-right"></span></label>');
         $('#slider2 a').html('<label><span class="glyphicon glyphicon-chevron-left"></span> '+$duration+' <span class="glyphicon glyphicon-chevron-right"></span></label>');

      }

    </script>

